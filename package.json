{
  "name": "to-do",
  "version": "1.5.1",
  "description": "Electron Todo application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ electron-builder --dir",
    "dist": "ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.8",
    "electron-builder": "^20.27.1"
  },
  "build": {
    "appId": "com.woolson.todo",
    "productName": "Microsoft To-do",
    "mac": {
      "icon": "build/icon.icns"
    },
    "dmg": {
      "window": {
        "x": 90,
        "y": 60,
        "width": 500,
        "height": 300
      }
    }
